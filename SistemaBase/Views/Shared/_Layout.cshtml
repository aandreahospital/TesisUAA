        <script>
            const navbar = document.querySelector('.navbar');
            const content = document.querySelector('.content');

            let isResizing = false;
            let startX = 0;
            let startNavbarWidth = 0;

            navbar.addEventListener('mousedown', function (e) {
                if (e.target.classList.contains('navbar') && e.offsetX >= navbar.offsetWidth - 10) {
                    isResizing = true;
                    startX = e.clientX;
                    startNavbarWidth = navbar.offsetWidth;

                    document.addEventListener('mousemove', resizeNavbarAndContent);
                    document.addEventListener('mouseup', stopResizeNavbarAndContent);
                }
            });

            function resizeNavbarAndContent(e) {
                if (!isResizing) return;

                const newNavbarWidth = startNavbarWidth + (e.clientX - startX);
                if (newNavbarWidth >= 200) { // Establece un ancho mínimo

                    // Aplica el nuevo ancho a la barra de navegación
                    navbar.style.width = newNavbarWidth + 'px';

                    // Calcula el ancho del contenido para que se ajuste a la pantalla
                    const windowWidth = window.innerWidth;
                    const contentWidth = windowWidth - newNavbarWidth;

                    // Asegura que el contenido no se contraiga hacia el lado contrario
                    content.style.marginLeft = newNavbarWidth + 'px';
                    content.style.width = contentWidth + 'px';
                }
            }

            function stopResizeNavbarAndContent() {
                isResizing = false;
                document.removeEventListener('mousemove', resizeNavbarAndContent);
                document.removeEventListener('mouseup', stopResizeNavbarAndContent);
            }



            // Función para actualizar la fecha y hora en tiempo real
            function actualizarFechaHora() {
                var fechaHoraActual = new Date();
                var opciones = { weekday: 'long', year: 'numeric', month: 'long', day: 'numeric', hour: 'numeric', minute: 'numeric', second: 'numeric' };
                var formatoFechaHora = fechaHoraActual.toLocaleDateString('es-ES', opciones);
                document.getElementById('fechaHora').textContent = formatoFechaHora;
            }

            // Actualizar la fecha y hora cada segundo
            setInterval(actualizarFechaHora, 1000);

            // Puedes utilizar esta función para actualizar dinámicamente el nombre de usuario si cambia
            function actualizarNombreUsuario() {
                // Obtener el nuevo nombre de usuario
                var nuevoNombreUsuario = ObtenerNuevoNombreUsuario();
                document.getElementById('nombreUsuario').textContent = nuevoNombreUsuario;
            }

            // Llama a la función para actualizar el nombre de usuario si cambia
            // actualizarNombreUsuario();

            // Función de ejemplo para obtener un nuevo nombre de usuario (ajusta según tu lógica)
            function ObtenerNuevoNombreUsuario() {
                return "NuevoUsuario";
            }



        </script>
