                var opciones = { weekday: 'long', year: 'numeric', month: 'long', day: 'numeric', hour: 'numeric', minute: 'numeric', second: 'numeric' };
                var formatoFechaHora = fechaHoraActual.toLocaleDateString('es-ES', opciones);
                document.getElementById('fechaHora').textContent = formatoFechaHora;
