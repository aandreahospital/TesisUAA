    <script>
        function loadPage(url) {
            // Realizar una solicitud AJAX al servidor
            $.ajax({
                url: url,
                type: 'GET',
                success: function (data) {
                    // Actualizar el contenedor de contenido con la respuesta
                    $('#dynamic-content-container').html(data);
                },
                error: function () {
                    console.error('Error cargando la página');
                }
            });

            // Evitar el comportamiento predeterminado del enlace (evitar recargar la página)
            return false;
        }
    </script>

