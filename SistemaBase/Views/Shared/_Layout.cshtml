        <script>
            const navbar = document.querySelector('.navbar');
            const content = document.querySelector('.content');

            let isResizing = false;
            let startX = 0;
            let startNavbarWidth = 0;

            navbar.addEventListener('mousedown', function (e) {
                if (e.target.classList.contains('navbar') && e.offsetX >= navbar.offsetWidth - 10) {
                    isResizing = true;
                    startX = e.clientX;
                    startNavbarWidth = navbar.offsetWidth;

                    document.addEventListener('mousemove', resizeNavbarAndContent);
                    document.addEventListener('mouseup', stopResizeNavbarAndContent);
                }
            });

            function resizeNavbarAndContent(e) {
                if (!isResizing) return;

                const newNavbarWidth = startNavbarWidth + (e.clientX - startX);
                if (newNavbarWidth >= 200) { // Establece un ancho mínimo

                    // Aplica el nuevo ancho a la barra de navegación
                    navbar.style.width = newNavbarWidth + 'px';

                    // Calcula el ancho del contenido para que se ajuste a la pantalla
                    const windowWidth = window.innerWidth;
                    const contentWidth = windowWidth - newNavbarWidth;

                    // Asegura que el contenido no se contraiga hacia el lado contrario
                    content.style.marginLeft = newNavbarWidth + 'px';
                    content.style.width = contentWidth + 'px';
                }
            }

