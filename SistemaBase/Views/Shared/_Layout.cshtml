        <script>
            // Función para manejar el clic en los enlaces desplegables
            function handleDropdownClick(event, id) {
                event.preventDefault();
                const dropdown = document.getElementById(id);
                const isExpanded = dropdown.classList.contains('show');

                if (isExpanded) {
                    dropdown.classList.remove('show');
                    localStorage.removeItem(id); // Elimina el registro del localStorage
                } else {
                    dropdown.classList.add('show');
                    localStorage.setItem(id, 'show'); // Almacena la configuración en el localStorage
                }
            }

            // Restaurar el estado de las listas desplegables al cargar la página
            document.addEventListener('DOMContentLoaded', function () {
                const dropdowns = document.querySelectorAll('.collapse.parent');

                dropdowns.forEach(dropdown => {
                    const id = dropdown.getAttribute('id');
                    const isExpanded = localStorage.getItem(id) === 'show';

                    if (isExpanded) {
                        dropdown.classList.add('show');
                    }
                });
            });
        </script>

